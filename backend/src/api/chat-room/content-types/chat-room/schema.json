{
  "kind": "collectionType",
  "collectionName": "chat_rooms",
  "info": {
    "singularName": "chat-room",
    "pluralName": "chat-rooms",
    "displayName": "Chat Room"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "roomId": {
      "type": "uid"
    },
    "name": {
      "type": "string"
    },
    "users": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "inversedBy": "chat_rooms"
    },
    "messages": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::message.message",
      "mappedBy": "chatRoom"
    }
  }
}
